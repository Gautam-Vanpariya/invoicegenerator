<html lang="en">
<head>
    <style>
        @font-face {
            font-family: 'Poppins';
            src: url('/fonts/Poppins-Regular.eot');
            src: url('/fonts/Poppins-Regular.eot?#iefix') format('embedded-opentype'),
                url('/fonts/Poppins-Regular.woff2') format('woff2'),
                url('/fonts/Poppins-Regular.woff') format('woff'),
                url('/fonts/Poppins-Regular.ttf') format('truetype'),
                url('/fonts/Poppins-Regular.svg#Poppins-Regular') format('svg');
            font-weight: normal;
            font-style: normal;
            font-display: swap;
        }
    </style>
</head>
<body style="font-family: 'Poppins', sans-serif;font-style: normal; margin: 0;">
    <table cellspacing="0" style="width: 100%; height:80px; font-style: normal;background:linear-gradient(90deg,rgba(223, 241, 255, .81) 58%, #fafcff 79%);">
        <tbody>
            <tr>   
                <% if(result.logo_path){  %>
                    <td id = "BillLogo_1" style="vertical-align: top; padding-left: 24px; padding-top: 17px; width:2%;">
                        <img src="<%= process.env.HOST%>/<%= result.logo_path %>" alt="logo" style="width: 40px; height: 40px;" /> 
                    </td> 
                <% } %> 
                <td style="width: 49%; vertical-align: top; padding-left: 18px; padding-top: 17px; padding-bottom: 13px;">
                    <h2 style="margin: 0; font-size: 28.7px; font-weight: 600; line-height: 32px; word-spacing: 1px; letter-spacing: -2px;">ENERGY STATEMENT</h2>
                    <h3 style="font-weight: normal; font-size: 17.5px; letter-spacing: -1px; margin: -7px 0 0;">
                        <%= result.company_website %>
                    </h3>
                </td>
                <td style="width: 49%; text-align: right; vertical-align: top; padding-right: 19px;">
                    <table cellspacing="0" style="width: 100%; vertical-align: top;">
                        <tbody>
                            <tr style="padding-bottom: 5px;">
                                <td align="right" style="width: 63%; font-size: 17px; padding-top: 8px; letter-spacing: -0.3px; line-height: 22px;">Account No:</td>
                                <td align="right" style="width: 37%; font-size: 17px; padding-top: 8px; line-height: 22px;">
                                    <%= data.application_number %>
                                </td>
                            </tr>
                            <tr style="padding-bottom: 5px;">
                                <td align="right" style="width: 63%; font-size: 17px; letter-spacing: -0.5px; line-height: 22px;">Statement Date:</td>
                                <td align="right" style="width: 37%; font-size: 17px; line-height: 22px;">
                                    <%= result.statement_date %>
                                </td>
                            </tr>
                            <tr>
                                <td align="right" style="width: 63%; font-size: 16.5px; letter-spacing: 0.3px; line-height: 22px;"><strong>Due Date:</strong></td>
                                <td align="right" style="width: 37%; font-size: 17px; line-height: 22px;"><strong><%= result.due_date %></strong></td>
                            </tr>
                        </tbody>
                    </table>
                </td>
            </tr>
        </tbody>
    </table>
    
    <table cellspacing="0" style="margin-top: 17px; width: 100%; margin: 0px 16px;">
        <tbody>
            <tr>
                <td style="width: 33.7%; vertical-align: top;">
                    <h3 style="font-size: 18px; letter-spacing: -0.5px; margin: 0; padding: 9px 0 0 8px;">Service For:</h3>
                    <div style="font-size: 12px; line-height: 17px; text-transform: uppercase; padding: 3px 0 0 8px;">
                        <span style="letter-spacing: 0.35px;">
                            <%= result.name %>
                        </span><br>
                        <span style="margin-top: -5px;">
                            <%= result.address %>
                                <%= result.user_state %>
                        </span>
                    </div>
                    <div style="background-color: #cccccd; margin-top: 40px; padding-bottom: 2px;">
                        <h2 style="font-size: 13.3px; font-weight: 600; line-height: 16px; margin: 0; padding: 4px 0 0 8px; word-spacing: 2px;">Questions about your bill?</h2>
                    </div>
                    <div style="font-size: 12.4px; letter-spacing: 0.1px; margin-top: 5px; padding-left: 8px;">Monday-Friday 7 a.m.-9 p.m.</div>
                    <div style="font-size: 12.4px; letter-spacing: 0.1px; line-height: 13px; padding-left: 8px; margin-top: -1px;">Saturday 8 a.m.-6 p.m.</div>
                    <div style="font-size: 12.4px; padding-top: 1px; padding-left: 8px; margin-top: -1px;">Phone: <span style="font-size: 11.5px;">1-800-743-5000</span></div>
                    <div style="font-size: 12.4px; line-height: 12px; padding-left: 8px; margin-top: -1px;"><%= result.company_website %></div>

                    <div style="background-color: #cccccd; margin-top: 13px;">
                        <h3 style="font-size: 13.3px; font-weight: 600; line-height: 16px; padding: 3px 0px 1px 5px; margin-bottom: 0; word-spacing: 2px;">Ways To Pay</h3>
                    </div>

                    <div style="font-size: 12px; letter-spacing: -0.2px; padding: 4px 4px 4px 8px;"><%= result.company_website %></div>
                </td>
                <td style="width: 66.3%; vertical-align: top; padding: 0px 10px 10px;">
                    <table cellspacing="0" style="width: 100%;">
                        <thead>
                            <tr>
                                <th>
                                    <h2 align="left" style="font-size: 16.5px; letter-spacing: 0.3px; margin: 8px 0 2px 0; padding-left: 9px;">Your Account Summary</h2>
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>
                                    <table style="width:100%; padding-right: 22px;" cellspacing="0">
                                        <tbody>
                                            <tr>
                                                <td style="width: 78%; font-size: 12px; line-height: 16px; padding-left: 9px;">Amount Due on Previous Statement</td>
                                                <td align="right" style="width: 22%; font-size: 12px; line-height: 16px;">$<%= result.previous_payment %>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td style="font-size: 12px; line-height: 16px; padding-left: 9px;">Payment(s) Received Since Last Statement</td>
                                                <td align="right" style="font-size: 12px; line-height: 16px;">- $<%= result.previous_payment %>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td></td>
                                                <td>
                                                    <hr style="border-bottom: 1px solid #000; margin-top: 1px; margin-bottom: 0;" />
                                                </td>
                                            </tr>
                                            <tr>
                                                <td style="font-size: 12px; line-height: 16px; word-spacing: 1px; letter-spacing: -0.2px; padding-left: 9px;">Previous Unpaid Balance</td>
                                                <td align="right" style="font-size: 12px; line-height: 16px;">$0.00</td>
                                            </tr>
                                            <tr>
                                                <td colSpan="2"></td>
                                            </tr>
                                            <tr>
                                                <td style="font-size: 12px; line-height: 16px; word-spacing: 1px; padding-left: 9px;">Current PG&E Electric Delivery Charges</td>
                                                <td align="right" style="font-size: 12px; line-height: 16px;">$<%= result.delivery_charges %>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td style="font-size: 12px; line-height: 16px; word-spacing: 1px; padding-left: 9px;">Silicon Valley Clean Energy Electric Generation Charges</td>
                                                <td align="right" style="font-size: 12px; line-height: 16px;">$<%= result.generation_charges %>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td colSpan="2" height="23"></td>
                                            </tr>
                                            <tr>
                                                <td colspan="2" style="padding-left: 8px;">
                                                    <table cellspacing="0" style="width: 100%; border: 1px solid #000;">
                                                        <tbody>
                                                            <tr style="vertical-align: top;">
                                                                <td style="width: 78%; font-size: 17px; padding: 3px 5px; margin-top: 10px;"><strong>Total Amount Due </strong>by <%= result.due_date %></td>
                                                                <td align="right" style="width: 22%; font-size: 17px; padding: 3px 5px;"><strong>$<%= result.due_payment_amt %></strong></td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </td>
                            </tr>
                            <tr>
                                <td colSpan="2" height="99"></td>
                            </tr>
                            <tr>
                                <td style="margin-right: 50px;">
                                    <table cellspacing="0" style="width: 95.5%; border: 1px solid #000;">
                                        <tbody>
                                            <tr style="background-color: #cccccd;">
                                                <td align="left">
                                                    <h2 style="font-size: 13.3px; font-weight: 600; line-height: 16px; padding: 2.5px 2px 2px 5px; margin: 0;">Monthly Billing History</h2>
                                                </td>
                                                <td align="right" style="font-size: 9px; padding-right: 5px;">Daily Usage Comparison</td>
                                            </tr>
                                            <tr>
                                                <td colSpan="2">
                                                    <img src="<%= process.env.HOST %>/<%= result.graph_path %>" alt="graph" style="object-fit:contain; width:100%; height:80px"/>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </td>
            </tr>
        </tbody>
    </table>
    <table cellspacing="0" style="width: 100%;">
        <tbody>
            <tr>
                <td style="padding: 0 24px;">
                    <h2 style="font-size: 13px; line-height: 8px; margin: 0;">Important Messages</h2>
                </td>
            </tr>
            <tr>
                <td style="padding: 0 18px 3px 24px;">
                    <p style="margin: 0; font-size: 11px; line-height: 16px; word-spacing: -0.3px; letter-spacing: 0.2px; padding-top: 9px;">
                        The Family Electric Rate Assistance (FERA) Program provides a monthly discount on electric bills for income-qualified households of three or more persons. To see if you qualify, please call <strong><%= result.service_number %></strong> or apply online at <strong style="font-size: 11px; letter-spacing: -0.2px;"><%= result.company_website %>.</strong>
                    </p>
                    <p style="margin: 0; font-size: 11px; line-height: 16px; word-spacing: -0.3px; letter-spacing: 0.2px; padding-top: 11px;">
                        El Programa FERA ofrece ahorros mensuales solo en las facturas de electricidad a hogares de ingresos economicos bajos y medianos con tres o mas personas. Para determinar si califica,por favor llame al <strong><%= result.service_number %></strong> o puede aplicar a traves de nuestra pagina web <strong style="font-size: 11px; letter-spacing: -0.2px;"> <%= result.company_website %>.</strong>
                    </p>
                </td>
            </tr> 
            <tr>
                <td style="border-bottom: 2px dotted #000;"></td>
            </tr>
            <tr>
                <td>
                    <h6 style="font-size: 10px; font-weight: 600; letter-spacing: 0; margin: 0; padding: 0 20px 0 50px;">
                        Please return this portion with your payment. No staples or paper clips. Do not fold. Thank you.
                    </h6>
                </td>
            </tr>
        </tbody>
    </table>
    <table style="width:100%;" cellspacing="0">
        <tbody>
            <tr>
                <td style="width: 10% ;padding: 66px 13px 0px 52px; vertical-align: top;">
                    <% if(result.logo_path){ %>
                        <img src="<%= process.env.HOST%>/<%= result.logo_path %>" alt="logo" style="width: 46px; height: 50px;" />
                    <% } %> 
                </td>
                <td style="width: 90%; padding: 65px 35px 0 9px">
                    <div style="border: 1px solid #000;">
                        <table cellspacing="0" style="width: 100%;">
                            <tbody>
                                <tr>
                                    <td style="vertical-align: top; width: 67%; padding-bottom: 26px;">
                                        <table cellspacing="0" style="width: 100%;">
                                            <tbody>
                                                <tr>
                                                    <td style="width: 31%; font-size: 11.7px; padding-top: 10px; padding-left: 3px;">Account Number:</td>
                                                    <td style="width: 31%; font-size: 11.7px; padding-top: 10px;">Due Date:</td>
                                                    <td style="width: 38%; font-size: 12px; letter-spacing: 0.2px; padding-top: 10px; padding-left: 8px;">Total Amount Due:</td>
                                                </tr>
                                                <tr>
                                                    <td style="font-size: 17.4px; line-height: 14px;letter-spacing: 0.3px; padding-left: 3px;">
                                                        <strong><%= data.application_number %></strong>
                                                    </td>
                                                    <td style="font-size: 17.4px; line-height: 14px;">
                                                        <strong><%= result.due_date %></strong>
                                                    </td>
                                                    <td style="font-size: 17.4px; line-height: 14px; padding-left: 8px;">
                                                        <strong>%<%= result.due_payment_amt %></strong>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </td>
                                    <td style="width: 33%; padding: 12px 12px 8px 3px;">
                                        <table cellspacing="0" style="border: 1px solid #c6c2cd; width: 100%;">
                                            <tbody>
                                                <tr>
                                                    <td colSpan="17" style="font-size: 12px; line-height: 0px; padding: 13px 5px 0px;">Amount Enclosed:</td>
                                                </tr>
                                                <tr>
                                                    <td style="width: 4%; font-size: 12px; padding-bottom: 5px; padding-left: 6px; vertical-align: bottom">
                                                        <b>$</b>
                                                    </td>
                                                    <td style="width: 11.75%;">
                                                        <ul style="margin: 0px; padding-left: 0px;">
                                                            <li
                                                                style="margin: 1px 3px; background-color: #c6c2cd; list-Style: none; height: 21px;">
                                                            </li>
                                                        </ul>
                                                    </td>
                                                    <td style="width: 11.75%;">
                                                        <ul style="margin: 0px; padding-left: 0;">
                                                            <li
                                                                style="margin: 1px 3px; background-color: #c6c2cd; list-Style: none; height: 21px;">
                                                            </li>
                                                        </ul>
                                                    </td>
                                                    <td style="width: 11.75%;">
                                                        <ul style="margin: 0px; padding-left: 0;">
                                                            <li
                                                                style="margin: 1px 3px; background-color: #c6c2cd; list-Style: none; height: 21px;">
                                                            </li>
                                                        </ul>
                                                    </td>
                                                    <td style="width: 11.75%;">
                                                        <ul style="margin: 0px; padding-left: 0;">
                                                            <li
                                                                style="margin: 1px 3px; background-color: #c6c2cd; list-Style: none; height: 21px;">
                                                            </li>
                                                        </ul>
                                                    </td>
                                                    <td style="width: 11.75%;">
                                                        <ul style="margin: 0px; padding-left: 0;">
                                                            <li
                                                                style="margin: 1px 3px; background-color: #c6c2cd; list-Style: none; height: 21px;">
                                                            </li>
                                                        </ul>
                                                    </td>
                                                    <td style="width: 11.75%;">
                                                        <ul style="margin: 0px; padding-left: 0;">
                                                            <li
                                                                style="margin: 0 3px; background-color: #c6c2cd; list-style: none; height: 21px;">
                                                            </li>
                                                        </ul>
                                                    </td>
                                                    <td style="width: 2%;">
                                                        <ul style="margin: 0px; padding-left: 0;">
                                                            <li
                                                                style="list-style: none; font-size: 18px; text-align: center">
                                                                <sub>
                                                                    <strong>.</strong>
                                                                </sub>
                                                            </li>
                                                        </ul>
                                                    </td>
                                                    <td style="width: 11.75%;">
                                                        <ul style="margin: 0px; padding-left: 0;">
                                                            <li
                                                                style="margin: 1px 3px; background-color: #c6c2cd; list-Style: none; height: 21px;">
                                                            </li>
                                                        </ul>
                                                    </td>
                                                    <td style="width: 11.75%">
                                                        <ul style="margin: 0px; padding-left: 0;">
                                                            <li
                                                                style="margin: 1px 3px; background-color: #c6c2cd; list-Style: none; height: 21px;">
                                                            </li>
                                                        </ul>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </td>
            </tr>
        </tbody>
    </table>
    <div style="padding: 40px 34px 75px 0;">
        <div style="float:right; font-size: 13px; line-height: 15px;">
            <span><%= result.company_name %></span>
            <br>
            <span><%= result.company_address %>,</span>
            <br>
            <span style="padding-bottom:30px; word-spacing: -0.5px; letter-spacing: 0.3px;">
                <%= result.company_city %>, <%= result.company_state %>
                        <%= result.company_zip %>
            </span>
        </div>
    </div>
</body>
</html>