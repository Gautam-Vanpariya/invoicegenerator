    <div class="header_top">
       <div class="container d-flex justify-content-end align-items-center">
         <div class="text-right">Email :  <a href="mailto:<%- process.env.EMAIL_USER %>" class="email_link"><%- process.env.EMAIL_USER %></a></div>
         <% if(typeof loggedInUser!=="undefined" && loggedInUser && loggedInUser._id) {%>
            <div class="user-dropdown d-inline-block">
               <button type="button" class="btn header-item waves-effect" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                  <img class="rounded-circle header-profile-user" src="/image/profile.png" alt="Profile">
                  <span class="d-none d-xl-inline-block ms-1" key="t-henry"><%=loggedInUser.user_name%></span>
                  <i class="fas fa-angle-down d-none d-xl-inline-block"></i>
               </button>
               <div class="dropdown-menu dropdown-menu-end">
                  <!-- item-->
                  <a class="dropdown-item" href="/myOrder"><span>My Order</span></a>
                  <a class="dropdown-item" href="/profile"><span>Profile</span></a>
                  <div class="dropdown-divider"></div>
                  <a class="dropdown-item text-danger" href="/auth/logout"><i class="bx bx-power-off font-size-16 align-middle me-1 text-danger"></i> <span key="t-logout">Logout</span></a>
               </div>
            </div>
          <% }else{%> 
            <div class="login_register">
               <ul class="navbar-nav flex-row align-items-center">
                  <li class="nav_item_top nav_item_top_login"><a class="nav_link_top text-uppercase" onclick="showAuthForm('Login')" href="javascript:void(0)">Login</a></li>
                  <li class="nav_item_top nav_item_top_register"><a class="nav_link_top text-uppercase" onclick="showAuthForm('Register')" href="javascript:void(0)">Register</a></li>
               </ul>
            </div>
          <% }%>  
       </div>
    </div>
    <div id="header" class="header_bottom">
       <div class="container">
          <nav class="navbar navbar-expand-lg py-0">
             <a aria-current="page" class="navbar-brand p-0 active" href="/"><img src="/image/logo/Bhavy Computer.png" class="header_logo" alt="bill generator"></a>
             <div class="d-none mobile-profile-wrapper">
               <div class="user-dropdown d-inline-block">
                  <button type="button" class="btn header-item waves-effect" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                     <img class="rounded-circle header-profile-user" src="/image/profile.png" alt="Profile">
                  </button>
                  <div class="dropdown-menu dropdown-menu-end">
                     <!-- item-->
                     <% if(typeof loggedInUser!=="undefined" && loggedInUser && loggedInUser._id) {%>
                        <a class="dropdown-item" href="/myOrder"><span>My Order</span></a>
                        <a class="dropdown-item" href="/profile"><span>Profile</span></a>
                        <div class="dropdown-divider"></div>
                        <a class="dropdown-item text-danger" href="/auth/logout"><i class="bx bx-power-off font-size-16 align-middle me-1 text-danger"></i> <span key="t-logout">Logout</span></a>
                     <% }else{%>
                        <ul class="align-items-center list-unstyled p-3 m-0 ">
                           <li><a class="text-uppercase text-decoration-none" onclick="showAuthForm('Login')" href="javascript:void(0)">Login</a></li>
                           <li class="mt-3"><a class="text-uppercase text-decoration-none" onclick="showAuthForm('Register')" href="javascript:void(0)">Register</a></li>
                        </ul>
                     <% }%>
                  </div>
               </div>
               <button class="navbar-toggler collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#navbarMain" aria-controls="navbarMain" aria-expanded="false" aria-label="Toggle navigation"><div id="toggle"><div class="one"></div><div class="two"></div><div class="three"></div></div></button>
             </div>
             <div class="collapse navbar-collapse navbar_collapse mobile-navbar" id="navbarMain">
               <ul class="navbar-nav mx-auto me-lg-0">
                  <li class="nav-item nav_item"><a aria-current="page" class="nav-link nav_link" href="/">Home </a></li>
                  <!-- <li class="nav-item nav_item dropdown">
                     <a class="nav-link nav_link dropdown-toggle" href="#" id="navbarSubMenu" role="button" data-bs-toggle="dropdown" aria-expanded="false">Residency Bill</a>
                     <ul class="dropdown-menu" aria-labelledby="navbarSubMenu">
                        <li><a class="dropdown-item" href="/utility-bill-generator">Utility Bill </a></li>
                     </ul>
                  </li> -->
                  <li class="nav-item nav_item"><a class="nav-link nav_link" href="/invoice-generator">Invoice</a></li>
               </ul>
             </div>
          </nav>
       </div>
    </div>
 </header>